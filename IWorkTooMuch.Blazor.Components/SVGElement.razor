
<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
     class="@Class" viewBox="0 0 16 16">
    @foreach (var path in Paths)
    {
        <path d="@path" />
    }
    @foreach (var path in EvenOddPaths)
    {
        <path fill-rule="evenodd" d="@path" />
    }
</svg>
